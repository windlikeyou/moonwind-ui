{
  "name": "@moonwind-ui/icons",
  "version": "1.0.0",
  "description": "Icons for Moonwind UI",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm generate && vite build && vue-tsc --project tsconfig.build.json",
    "generate": "esno scripts/generate.ts",
    "dev": "vite build --watch"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0"
  }
}
